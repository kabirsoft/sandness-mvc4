                                            case 2:
                                                feltval = db.QueryValue("SELECT name FROM [SolaBetong].[dbo].[dropdowns] WHERE id=@0", @ordv_data.choice);
                                                break;

                                            case 3:
                                                feltval = db.QueryValue("SELECT name FROM [SolaBetong].[dbo].[choices] WHERE  id=@0", @ordv_data.choice);
