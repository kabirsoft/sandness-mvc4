                          var page_status3_style = "";
                          if (@i == @page3)
                          {
                              page_status3_style = "page_status3";
                          }
                          else
                          {
                              page_status3_style = "";
                          }
                          <li><a  class="@page_status3_style" href="/bestillinger/index/?paging_date_search=@date_bestillinger&paging_st3=status3_@i#status_3">@i</a></li>
